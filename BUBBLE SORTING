## using iterative method

n=8
lis=[1,4,3,2,4,9,7,8]
for i in range(n):
    for j in range(n-i-1):
        if lis[j]>lis[j+1]:
            lis[j],lis[j+1]=lis[j+1],lis[j]
print(lis)



##using functions

def bubblesort(lis):
    for i in range(n):
        for j in range(n-i-1):
            if lis[j+1]<lis[j]:
                lis[j],lis[j+1]=lis[j+1],lis[j]
    return lis
n=8
lis=[1,4,3,2,4,9,7,8]
print(bubblesort(lis))
